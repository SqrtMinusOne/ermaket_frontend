{
  "root": true,
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "plugin:vue/essential",
    "@vue/airbnb",
    "@vue/typescript",
    "prettier"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "parser": "@typescript-eslint/parser",
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "vue"
  ],
  "rules": {
    "class-methods-use-this": 0,
    "no-unused-vars": 0,
    "lines-between-class-members": 0,
    "no-console": 0,
    "no-plusplus": 0,
    "prefer-destructuring": 0,
    "import/first": 0,
    "import/newline-after-import": 0,
    "import/order": 0,
    "no-restricted-syntax": 0
  }
}
